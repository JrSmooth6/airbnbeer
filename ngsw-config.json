{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/assets/img/LOGO128.ico",
          "/assets/img/LOGO256.ico",
          "/*.(png|jpg|jpeg|gif|svg)"
        ]
      }
    }
  ]
}
